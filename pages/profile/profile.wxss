.content{
  width: 750rpx;
  height: 100vh;
  background-color: #F0EFF4;
}
.head{
  width: 100%;
  height: 390rpx;
  background-color: #CD9057;
  padding-top: 30rpx
}
.head .touxiang{
  width: 180rpx;
  height: 180rpx;
  
  display: block;
  border-radius: 100%;
  margin: 0 auto;
}
.name{
  padding-top: 20rpx;
  text-align: center
}
.item{
  margin-top: 4rpx;
  width: 650rpx;
  height: 60rpx;
  background-color: white;
  padding: 10rpx 50rpx;
  line-height: 60rpx;
  font-size: 34rpx 
}
.item image{
  width: 60rpx;
  height: 60rpx;
  float: left;
}

.item text{
  margin-left: 30rpx;
  float: left;
}
.item .jiantou{
  padding: 10rpx 0;
  width: 40rpx;
  height: 40rpx;
  float: right;
}

.item1 image{
  padding: 5rpx 0;
  width: 50rpx;
  height: 50rpx
}
.item2{
  margin-top: 40rpx
}